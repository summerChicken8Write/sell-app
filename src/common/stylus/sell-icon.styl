@font-face {
  font-family: 'sell-icon';
  src:  url('../fonts/sell-icon.eot?ovpyms');
  src:  url('../fonts/sell-icon.eot?ovpyms#iefix') format('embedded-opentype'),
    url('../fonts/sell-icon.ttf?ovpyms') format('truetype'),
    url('../fonts/sell-icon.woff?ovpyms') format('woff'),
    url('../fonts/sell-icon.svg?ovpyms#sell-icon') format('svg');
  font-weight: normal;
  font-style: normal;
}

.icon {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'sell-icon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

